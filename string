public class PrintNames {  
    public static void main(String[] args) {  
   
        String[] names = {"Ali", "Sara", "Reza", "Mahnaz", "John"};  

     
        System.out.println("List of names:");  
        for (int i = 0; i < names.length; i++) {  
            System.out.println(names[i]);  
        }  
    }  
}
